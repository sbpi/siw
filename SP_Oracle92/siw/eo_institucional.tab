create table EO_INSTITUCIONAL
(
  SQ_UNIDADE       NUMBER(10) not null,
  MISSAO           VARCHAR2(2000) not null,
  VISAO_FUTURO     VARCHAR2(2000),
  AMBIENTE_EXTERNO VARCHAR2(2000),
  VALORES          VARCHAR2(2000)
)
tablespace SYSTEM
  pctfree 10
  pctused 40
  initrans 1
  maxtrans 255
  storage
  (
    initial 66K
    minextents 1
    maxextents unlimited
  );
comment on table EO_INSTITUCIONAL
  is 'Registra as informações da unidade relativas à sua participação na organização.';
comment on column EO_INSTITUCIONAL.SQ_UNIDADE
  is 'Sequence.';
comment on column EO_INSTITUCIONAL.MISSAO
  is 'Identificação da missão da unidade e seus atributos principais.';
comment on column EO_INSTITUCIONAL.VISAO_FUTURO
  is 'Visão desejada para a unidade considerando a constante mudança do ambiente em que a organização atua.';
comment on column EO_INSTITUCIONAL.AMBIENTE_EXTERNO
  is 'Relato do ambiente externo à organização onde a unidade pode e deve contribuir.';
comment on column EO_INSTITUCIONAL.VALORES
  is 'Valores que a unidade declara praticar, complementares aos valores da organização.';
alter table EO_INSTITUCIONAL
  add constraint PK_EO_INSTITUCIONAL primary key (SQ_UNIDADE)
  using index 
  tablespace SYSTEM
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 66K
    minextents 1
    maxextents unlimited
  );
alter table EO_INSTITUCIONAL
  add constraint FK_EOINS_EOUNI foreign key (SQ_UNIDADE)
  references EO_UNIDADE (SQ_UNIDADE);

