comment on column PJ_ETAPA_DEMANDA.SQ_SIW_SOLICITACAO
  is 'Chave de SIW_SOLICITACAO';
alter table PJ_ETAPA_DEMANDA
  add constraint PK_PJ_ETAPA_DEMANDA primary key (SQ_ETAPA_DEMANDA)
  using index 
