create table OR_ACOES
(
  PROGRAMA   VARCHAR2(50),
  ACAO       VARCHAR2(50),
  DESCRICAO  VARCHAR2(80),
  AUTORIZADO NUMBER(18,2),
  EMPENHADO  NUMBER(18,2),
  LIQUIDADO  NUMBER(18,2)
)
tablespace SYSTEM
  pctfree 10
  pctused 40
  initrans 1
  maxtrans 255
  storage
  (
    initial 72K
    next 64K
    minextents 1
    maxextents unlimited
    pctincrease 50
  );

